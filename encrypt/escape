    <script type="text/javascript">        function change() {
            var src = jQuery("#content").val();
            jQuery("#content").val(jQuery("#result").val());
            jQuery("#result").val(src);
        }
        function Escape_encode() {
            jQuery("#result").val(escape(jQuery("#content").val()));
        }
        function Escape_decode() {
            jQuery("#result").val(unescape(jQuery("#content").val()));
        }

        function Empty() {
            document.getElementById('content').value = '';
            document.getElementById('result').value = '';
            document.getElementById('content').select();
        }

        function GetFocus() {
            document.getElementById('content').focus();
        }
    </script>
