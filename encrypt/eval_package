nt(c / a))) + ((c = c % a) >35 ? String.fromCharCode(c + 29) : c.toString(36));        }        function run() {            eval(document.getElementById('code').value);        }        function decode() {            var code = document.getElementById('code').value;            code = code.replace(/^eval/, '');            document.getElementById('code').value = eval(code);        }        function Empty() {            document.getElementById('code').value = '';            document.getElementById('code').select();        }        function GetFocus() {            document.getElementById('code').focus();        }    </script>    <script>var _hmt = _hmt || [];(function () {var hm = document.createElement("script");hm.src = "https://hm.baidu.com/hm.js?adebc3d0ae83e7a888cd5f472c16164a";var s = document.getElementsByTagName("script")[0];s.parentNode.insertBefore(hm, s);})();</script></body></html>